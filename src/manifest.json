{
    "name": "DAX / iStats log",
    "version": "0.5.3",
    "description": "Logs calls to DAX and iStats",
    "options_page": "options.html",
    "permissions": ["webRequest", "webRequestBlocking", "*://*.bbc.co.uk/*", "activeTab", "storage"],
    "background": {
        "scripts": ["js/background.js"]
    },
    "manifest_version": 2,
    "browser_action": {
        "default_title": "",
        "default_icon": "img/logo.png"
    },
    "icons": { "48": "img/logo.png" }
}
